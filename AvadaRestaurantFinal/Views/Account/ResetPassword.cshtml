@using AvadaRestaurantFinal.ViewModels
@model ForgetPassword
@{
    Layout = null;
}

<div class="container">
    <div class="row">
        <form method="post">
            <input hidden asp-for="User" />
            <input hidden asp-for="Token" />
            <input asp-for="User.PasswordHash" />
            <button class="btn btn-primary" type="submit">Reset Email</button>
        </form>
    </div>
</div>
